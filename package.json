{
    "name": "tinge_of_magic",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npm run css_prod; webpack build -c webpack.prod.js --progress --color",
        "serve": "webpack serve -c webpack.dev.js  --progress --color",
        "host": "webpack serve -c webpack.prod.js --mode production",
        "clean": "rm app/static/compiled/* ",
        "lint": "eslint . --fix --quiet",
        "css:dev": "tailwindcss -i ./tailwind/tailwind.css -c ./tailwind/tailwind.config.js --postcss=./tailwind/postcss.config.js -o app/static/css/main.css",
        "css:watch": "tailwindcss -i ./tailwind/tailwind.css -c ./tailwind/tailwind.config.js --postcss=./tailwind/postcss.config.js -o app/static/css/main.css --watch",
        "css:prod": "tailwindcss -i ./tailwind/tailwind.css -c ./tailwind/tailwind.production.js --postcss=./tailwind/postcss.config.js -o app/static/css/main.css"
    },
    "keywords": [],
    "author": "Graham Abbott",
    "license": "None",
    "devDependencies": {
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "@wasm-tool/wasm-pack-plugin": "^1.6.0",
        "autoprefixer": "^10.4.2",
        "css-loader": "^6.2.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "html-webpack-plugin": "^5.3.1",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "prop-types": "^15.7.2",
        "style-loader": "^3.0.0",
        "tailwindcss": "^3.0.15",
        "text-encoding": "^0.7.0",
        "ts-loader": "^9.2.3",
        "typescript": "^4.2.4",
        "wasm-loader": "^1.3.0",
        "webpack": "^5.44.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^4.2.1"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
