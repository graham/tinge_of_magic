{
    "name": "tinge_of_magic",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "init": "npm run clean ; npm run css:dev ; npm run pages:genhtml",
        "clean": "rm -rf app/static/compiled/* ",
        "build": "npm run clean ; npm run css:release ; webpack build -c webpack.release.js --progress --color",
        "webserve": "webpack serve -c webpack.dev.js  --progress --color",
        "serve": "concurrently --raw \"npm run css:watch\" \"npm run webserve\"",
        "esserve": "concurrently --raw \"npm run css:watch\" \"tsc --noEmit --watch | sed -u 's/\\x1Bc//g'\" \"./build.js --watch --server\"",
        "host": "webpack serve -c webpack.release.js --mode production",
        "lint": "eslint app --fix --quiet",
        "css:watch": "tailwindcss -i ./tailwind/tailwind.css -c ./tailwind/tailwind.config.js --postcss=./tailwind/postcss.config.js -o app/static/css/tailwind_generated.css --watch",
        "css:release": "tailwindcss -i ./tailwind/tailwind.css -c ./tailwind/tailwind.production.js --postcss=./tailwind/postcss.config.js -o app/static/css/tailwind_generated.css",
        "pages:genhtml": "node ./lib/bin_gen_html.js --key=`git rev-parse HEAD`",
        "pages:startpage": "node ./lib/bin_create_new_page.js",
        "pages:doctor": "node ./lib/bin_doctor.js",
        "endtoend": "rm -rf node_modules/; npm install; npm run clean; npm run css:release; npm run pages:genhtml; npm run build"
    },
    "keywords": [],
    "author": "Graham Abbott",
    "license": "None",
    "devDependencies": {
        "@types/react": "^18.0.10",
        "@types/react-dom": "^18.0.5",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@wasm-tool/wasm-pack-plugin": "^1.6.0",
        "autoprefixer": "^10.4.7",
        "concurrently": "^7.2.1",
        "css-loader": "^6.7.1",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "html-webpack-plugin": "^5.5.0",
        "inquirer": "^8.2.4",
        "liquidjs": "^9.37.0",
        "postcss": "^8.4.14",
        "postcss-cli": "^9.1.0",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.0.24",
        "text-encoding": "^0.7.0",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.2",
        "wasm-loader": "^1.3.0",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1",
        "yargs": "^17.5.1",
        "@types/mousetrap": "^1.6.9",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4",
        "esbuild": "^0.14.43",
        "mousetrap": "^1.6.5",
        "react-router-dom": "^6.3.0"
    },
    "dependencies": {
    }
}
